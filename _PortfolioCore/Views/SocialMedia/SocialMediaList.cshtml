@model List<SocialMedia>

@{
    ViewData["Title"] = "SocialMediaList";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h3>Social Media List</h3>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Title</th>
        <th>Url</th>
        <th>Icon</th>
        <th>Update</th>
        <th>Delete</th>
        <th>Status</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.SocialMediaId</td>
            <td>@item.SocialMediaName</td>
            <td>@item.SocialMediaUrl</td>
            <td>@item.SocialMediaIcon</td>
            <td>@item.SocialMediaStatus</td>
            <td><a href="/SocialMedia/UpdateSocialMedia/@item.SocialMediaId" class=" btn btn-outline-success">Update</a></td>
            <td><a href="/SocialMedia/DeleteSocialMedia/@item.SocialMediaId" class=" btn btn-outline-danger">Delete</a></td>
        </tr>
    }
</table>
<a href="/SocialMedia/CreateSocialMedia/" class="btn btn-outline-primary col-md-12">New Social Media Login</a>
