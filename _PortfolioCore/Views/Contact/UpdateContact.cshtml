@model Contact
@{
    ViewData["Title"] = "UpdateContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h3>Contact Update Page</h3>
<br />
<form method="post">

    <input type="hidden" asp-for="ContactId" />

    <label>Address</label>
    <input type="text" asp-for="Address" class="form-control" style="border:1px solid grey;" />
    <br />

    <label>Phone Number</label>
    <input type="text" asp-for="PhoneNumber" class="form-control" style="border:1px solid grey;" />
    <br />

    <label>Email</label>
    <input type="text" asp-for="Email" class="form-control" style="border:1px solid grey" />
    <br />

    <label>Location</label>
    <input type="text" asp-for="Location" class="form-control" style="border:1px solid grey" />
    <br />
    <button class="btn btn-outline-primary col-md-12">Save</button>
</form>