@model List<Skill>
@{
    ViewData["Title"] = "SkillList";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<h2>Skill List</h2>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Skill Title</th>
        <th>Talent Value</th>
        <th>Delete</th>
        <th>Update</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.SkillId</td>
            <td>@item.SkillName</td>
            <td>@item.SkillValue</td>
            <td><a href="/Skill/DeleteSkill/@item.SkillId" class="btn btn-danger">Delete</a></td>
            <td><a href="/Skill/UpdateSkill/@item.SkillId" class="btn btn-success">Update</a></td>
        </tr>
    }
</table>
<a href="/Skill/CreateSkill/" class="btn btn-primary">New Talent Entry</a>